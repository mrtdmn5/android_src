<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="ring"
            android:thicknessRatio="@dimen/ring_thickness_ratio"
            android:useLevel="false">
            <solid android:color="@color/ring_progress_background" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90">
            <shape
                android:shape="ring"
                android:thicknessRatio="@dimen/ring_thickness_ratio"
                android:useLevel="true">
                <solid android:color="@color/ring_progress_progress_tone_1" />
            </shape>
        </rotate>
    </item>
</layer-list>
