    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/default_margin"
        tools:context=".fragments.PlotterFragment" android:tag="layout/fragment_music_processing_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

        <TextView
            android:id="@+id/errorTextView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            tools:visibility="gone" />

        <LinearLayout
            android:id="@+id/eqContainer"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/presetRowsContainerView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginBottom="@dimen/default_margin_one_and_half" android:tag="binding_1">

                <include layout="@layout/fragment_music_processing_btn_row" />

                <include layout="@layout/fragment_music_processing_btn_row" />

                <include layout="@layout/fragment_music_processing_btn_row" />

            </LinearLayout>

            <com.qualcomm.qti.gaiaclient.ui.settings.musicprocessing.plotting.GraphView
                android:id="@+id/graphView"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                app:layout_constraintDimensionRatio="0.5" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/default_margin_one_and_half"
                android:layout_weight="3" android:tag="binding_2">

                <include
                    android:id="@+id/labels"
                    layout="@layout/fragment_music_processing_labels"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <LinearLayout
                    android:id="@+id/sliderHorizontalContainer"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/eq_sliders_padding_left"
                    android:baselineAligned="false"
                    android:gravity="center"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" android:tag="binding_3">

                    <include layout="@layout/fragment_music_processing_slider" />

                    <include layout="@layout/fragment_music_processing_slider" />

                    <include layout="@layout/fragment_music_processing_slider" />

                    <include layout="@layout/fragment_music_processing_slider" />

                    <include layout="@layout/fragment_music_processing_slider" />

                    <include layout="@layout/fragment_music_processing_slider" />

                    <include layout="@layout/fragment_music_processing_slider" />

                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

    </FrameLayout>

         