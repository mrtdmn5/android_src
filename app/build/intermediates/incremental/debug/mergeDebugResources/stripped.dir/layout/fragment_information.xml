    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent" android:tag="layout/fragment_information_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:contentDescription="@string/cont_desc_information_screen"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:padding="@dimen/default_margin">

            <ImageView
                android:id="@+id/device_image"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="114dp"
                android:layout_marginTop="@dimen/default_margin_one_and_half"
                android:layout_marginBottom="@dimen/no_margin"
                android:tag="binding_1"                                                                                                                                      
                android:src="@drawable/ic_device_headphones"
                                                                         />

            <TextView
                android:id="@+id/device_name"
                style="@style/AppTheme.Text.Title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/default_margin_one_and_half"
                android:layout_marginBottom="@dimen/no_margin"
                android:tag="binding_2"                                                                            
                                                  
                android:textAlignment="center"
                                                                                            />

            <TextView
                android:id="@+id/device_variant_name"
                style="@style/AppTheme.Text.Subtitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/default_margin"
                android:layout_marginBottom="@dimen/no_margin"
                android:tag="binding_3"                                                                                           
                                                         
                android:textAlignment="center"
                                                                                                   />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/default_margin"
                android:layout_marginBottom="@dimen/no_margin"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:tag="binding_4"                                                          >

                <TextView
                    android:id="@+id/device_battery_level_title"
                    style="@style/AppTheme.Text.Subtitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:text="@string/battery_levels"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/device_battery_level_device"
                    style="@style/AppTheme.Text.Subtitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_5"                                             
                    android:textAlignment="center"
                                                                                                                                  />

                <TextView
                    android:id="@+id/device_battery_level_left"
                    style="@style/AppTheme.Text.Subtitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_6"                                           
                    android:textAlignment="center"
                                                                                                                                />

                <TextView
                    android:id="@+id/device_battery_level_right"
                    style="@style/AppTheme.Text.Subtitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_7"                                            
                    android:textAlignment="center"
                                                                                                                                 />

                <TextView
                    android:id="@+id/device_battery_level_case"
                    style="@style/AppTheme.Text.Subtitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_8"                                            
                    android:textAlignment="center"
                                                                                                                                 />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/default_margin"
                android:layout_marginBottom="@dimen/no_margin"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:tag="binding_9"                                                          >

                <TextView
                    android:id="@+id/device_serial_number_title"
                    style="@style/AppTheme.Text.Subtitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:text="@string/serial_number"
                    android:textAlignment="center" />

                <TextView
                    android:id="@+id/device_serial_number"
                    style="@style/AppTheme.Text.Subtitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_10"                                                                                            
                                                              
                    android:textAlignment="center"
                                                                                                            />

                <TextView
                    android:id="@+id/device_serial_number_earbud_left"
                    style="@style/AppTheme.Text.Subtitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_11"                                                                                                     
                                                                                                             
                    android:textAlignment="center"
                                                                                                            />

                <TextView
                    android:id="@+id/device_serial_number_earbud_right"
                    style="@style/AppTheme.Text.Subtitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_12"                                                                                                       
                                                                                                               
                    android:textAlignment="center"
                                                                                                             />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/default_margin_double"
                android:layout_marginBottom="@dimen/no_margin"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:tag="binding_13"                                                                                            >

                <TextView
                    android:id="@+id/device_address"
                    style="@style/AppTheme.Text.Message"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_14"                                                                                          
                                                                                                            
                    android:textAlignment="center"
                                                                                                            />

                <TextView
                    android:id="@+id/device_application_version"
                    style="@style/AppTheme.Text.Message"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/default_margin_half"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_15"                                                                                                        
                                                                                                                
                    android:textAlignment="center"
                                                                                                              />

                <TextView
                    android:id="@+id/device_application_build_id"
                    style="@style/AppTheme.Text.Message"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/default_margin_half"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_16"                                                                                                         
                                                                                                                 
                    android:textAlignment="center"
                                                                                                              />

                <TextView
                    android:id="@+id/device_gaia_version"
                    style="@style/AppTheme.Text.Message"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/default_margin_half"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_17"                                                                                          
                                                                                                  
                    android:textAlignment="center"
                                                                                                       />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/default_margin_double"
                android:layout_marginBottom="@dimen/no_margin"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:tag="binding_18"                                                  >

                <TextView
                    android:id="@+id/connection_state"
                    style="@style/AppTheme.Text.Message"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/no_margin"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:tag="binding_19"                                                                             
                                                       
                    android:textAlignment="center"
                                                                                                 />

                <TextView
                    android:id="@+id/device_charging"
                    style="@style/AppTheme.Text.Message"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/default_margin_half"
                    android:layout_marginBottom="@dimen/no_margin"
                    android:contentDescription="@string/cont_desc_information_is_charging"
                    android:text="@string/charging"
                    android:textAlignment="center"
                    android:tag="binding_20"                                                  />

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

         